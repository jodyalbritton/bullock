<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>Charlie Bullock Art Studio</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
  <meta name="description" content="">
  <meta name="author" content="">

  <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!--[if lt IE 9]>
    <%= javascript_include_tag "https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js", "https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js" %>
  <![endif]-->

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="contenedor">
    <div id="cabeza">
      <div class="navbar navbar-default">
        <div id="titulo" class="navbar text-center">
        <h1>Charlie Bullock Art Studio</h1>
        </div>
      </div>
      
      <div id="main-nav" class="navbar navbar-default container">
        
        <div class="offset-help">
          
          <div class="toggle-cont text-center">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            </button>
          </div>
          
          <div class="navbar-collapse collapse">
            <ul id="menu" class="nav navbar-nav text-center">

              <li><%= link_to "Home", root_url %></li>
              <li><%= link_to "Works", works_url %></li>
              <li><%= link_to "About", page_path('about') %></li>
              <li><%= link_to "Contact", page_path('contact') %></li>
        
            </ul>
                      
          </div>
          
          
        </div>  
      </div>
      
    </div>
    <div id="body" class="container">

      <%= yield %>
      
    </div>
    <div id="footer">
      <div class="container">
      
        <div class="offset-help">
        
          <div class="row">
            
            <div class="col-md-8 col-sm-8">
              <div class="menu-footer">
                <ul>
                <li class="active"><%= link_to "Home", root_url %></li>
                <li class="active"><%= link_to "About", page_path('about') %></li>
                <li class="active"><%= link_to "Contact", page_path('contact') %></li>
                </ul>
              </div>
              
              <div>
                <ul class="contact">
                <li><span class="glyphicon glyphicon-map-marker"></span>  120 E. Larkin, Athens, Texas, 75751</li>
                <li><span class="glyphicon glyphicon-phone-alt"></span>     903-780-2624 </li>
                <li><span class="glyphicon glyphicon-envelope"></span>   charliebullocktx@gmail.com</li>
                </ul>
              </div>
              
            </div>

            <div class="col-md-4 col-sm-4">
                <div class="menu-footer social">
                  <ul>
                  <li>Social links</li></ul>
                </div>
                
                <div class="social-icons">
                  <a href="facebook.com"><i class="fa fa-facebook-square"></i></a>
                  <a href="twitter.com"><i class="fa fa-twitter-square"></i></a>
                  <a href="youtube.com"><i class="fa fa-youtube-play"></i></a>
                  <a href="flickr.com"><i class="fa fa-flickr"></i></a>
                  <a href="instagram.com"><i class="fa fa-instagram"></i></a>
                  <a href="#"><i class="fa fa-rss-square"></i></a>
                </div>
                
            </div>
            
          </div>
          
          <div id="credit">
            <ul>
                <li>Â© 2014 Charlie Bullock Art Studio. All Rights Reserved</li>
              <li>Design By Optimus Labs <a href="http://getoptimus.com">Get Optimus</a> </li>
              <% if signed_in? %>
                <li><%= link_to('Edit registration', edit_user_registration_path) %></li>
                <li>
                 <%= link_to('Logout', destroy_user_session_path, :method => :delete) %>
                </li>
                <% else %>
                <li><%= link_to('Sign In', new_user_session_path)  %> </li>
                <li><%= link_to('Register', new_user_registration_path)  %> </li>
                <% end %>
              
            </ul>
          </div>
          
        </div>
        
      </div>
    </div>
  </div>
   <script type="text/javascript">
      //Preload images
      App.preloadImg();
      
      $(document).ready(function(){
        //initialize the javascript
        App.init();
      });
    </script>
</body>
</html>
